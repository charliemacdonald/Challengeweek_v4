<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.ipmedt4.challengeweek_v2.Versiescherm">

    <TextView android:text="Dit is versie 1.0. Hieraan kunnen geen rechten worden ontleend"
        android:textColor="#000000"
        android:textSize="22sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="93dp"
        android:layout_below="@+id/hsleidenlogo"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/hsleidenlogo"
        android:src="@drawable/logohsleiden"
        android:layout_width="50dp"
        android:layout_height="55dp" />

</RelativeLayout>
